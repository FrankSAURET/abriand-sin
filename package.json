{
	"name": "abriand-sin",
	"displayName": "ABriand SIN",
	"description": "Pack d'extension à destination des élèves de TSTI2D SIN du lycée Aristide Briand.",
	"version": "1.0.0",
	"author": {
		"name": "Frank SAURET",
		"email": "frank.sauret.prof@gmail.com",
		"url": "http://www.electropol.fr/"
	},
	"publisher": "electropol-fr",
	"repository": {
		"type": "git",
		"url": "https://github.com/FrankSAURET/abriand-sin"
	},
	"license": "MIT",
	"engines": {
		"vscode": "^1.46.0"
	},
	"icon": "image/LogoAbriandSIN.png",
	"keywords": [
		"python",
		"french",
		"Qt",
		"PyQt"
	],
	"categories": [
		"Extension Packs",
		"Snippets"
	],
	"extensionPack": [
		"aaron-bond.better-comments",
		"dongli.python-preview",
		"KevinRose.vsc-python-indent",
		"MS-CEINTL.vscode-language-pack-fr",
		"ms-python.python",
		"ms-python.vscode-pylance",
		"VisualStudioExptTeam.vscodeintellicode",
		"zhoufeng.pyqt-integration",
		"njpwerner.autodocstring",
		"epivision.vscode-file-header",
		"MS-vsliveshare.vsliveshare"
	],
	"activationEvents": [
		"onCommand:abriand-sin.configurer"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"snippets": [
			{
				"language": "python",
				"path": "./snippets/python.json"
			}
		],
		"commands": [
			{
				"command": "abriand-sin.configurer",
				"title": "abriand-sin : Configurer vs code pour le travail en SIN"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"compile-tests": "tsc -p . --outDir out",
		"watch-tests": "tsc -p . -w --outDir out",
		"pretest": "npm run compile-tests && npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.46.0",
		"@types/glob": "^7.1.4",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"@typescript-eslint/eslint-plugin": "^5.1.0",
		"@typescript-eslint/parser": "^5.1.0",
		"eslint": "^8.1.0",
		"glob": "^7.1.7",
		"mocha": "^9.1.3",
		"typescript": "^4.4.4",
		"ts-loader": "^9.2.5",
		"webpack": "^5.52.1",
		"webpack-cli": "^4.8.0",
		"@vscode/test-electron": "^1.6.2"
	},
	"dependencies": {
		"fso": "^2.7.0"
	}
}
